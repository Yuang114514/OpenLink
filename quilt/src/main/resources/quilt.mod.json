{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "openlink",
    "version": "${version}",
    "metadata": {
      "name": "OpenLink",
      "description": "Bring you a unique Minecraft Multiplayer playing experience OvO",
      "contributors": {
        "StarCarefree": "Team",
        "Terry_MC": "Author",
        "zirranYa": "Mastermind"
      },
      "icon": "icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "fun.moystudio.openlink.quilt.OpenLinkQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.21.4"
      }
    ]
  },
  "mixin": [
    "openlink.mixins.json"
  ],
  "license": "MIT"
}
